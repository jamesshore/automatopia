{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "indent": [2, 2],
    "brace-style": [2, "1tbs"],
    "comma-style": [2, "last"],
    "camelcase": 2,
    "space-in-parens": [2, "never"],
    "keyword-spacing": [2],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "consistent-return": 2,
    "consistent-this": [2, "self"],
    "no-bitwise": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-void": 2,
    "one-var": [2, "never"],
    "radix": 2,
    "no-throw-literal": 2,
    "wrap-iife": 2
    },
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "expect": false
  }
}
